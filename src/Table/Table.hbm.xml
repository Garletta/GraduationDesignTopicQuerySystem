<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Table.User" table="USER" schema="Graduation">
        <id name="account" column="ACCOUNT"/>
        <property name="password" column="PASSWORD"/>
        <property name="student" column="STUDENT"/>
    </class>
    <class name="Table.Design" table="DESIGN" schema="Graduation">
        <id name="student" column="STUDENT"/>
        <property name="topic" column="TOPIC"/>
        <property name="college" column="COLLEGE"/>
    </class>
</hibernate-mapping>